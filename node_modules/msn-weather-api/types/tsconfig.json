{
    "compilerOptions": {
        "module": "CommonJS",
        "lib": [
            "ES2022"
        ],
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true
    },
    "include": [
        "*.d.ts"
    ]
}